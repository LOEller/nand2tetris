class Main {
    function void main() {
        var SnakeGame game;

        do Screen.setColor(true);
        do Screen.drawLine(0, 14, 510, 14);

        let game = SnakeGame.new();
        do game.run();
        do game.dispose();

        do Output.moveCursor(10, 25);
        do Output.printString("Game Over!");
        return;
    }
}
